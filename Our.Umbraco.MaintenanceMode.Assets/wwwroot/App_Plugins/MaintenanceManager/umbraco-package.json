{
    "$schema": "../umbraco-package-schema.json",
    "name": "MaintenanceManager",
    "id": "Our.Umbraco.MaintenanceMode",
    "version": "14.0.0",
    "extensions": [
        {
            "type": "dashboard",
            "alias": "maintenance.dashboard",
            "name": "maintenanceDashboard",
            "js": "/App_Plugins/MaintenanceManager/maintenancemanager.js",
            "meta": {
                "label": "Maintenance Manager",
                "pathname": "maintenance-dashboard"
            },
            "conditions": [
                {
                    "alias": "Umb.Condition.SectionAlias",
                    "match": "Umb.Section.Content"
                }
            ]
        }
    ]
}